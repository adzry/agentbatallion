{
  "name": "@meta/agent-battalion",
  "version": "3.0.0",
  "description": "AI-powered full-stack app generator system with multi-agent collaboration",
  "main": "dist/web/server.js",
  "types": "dist/web/server.d.ts",
  "type": "module",
  "bin": {
    "agent-battalion": "./dist/cli/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/web/server.ts",
    "build": "tsc",
    "start": "node dist/web/server.js",
    "cli": "tsx src/cli/index.ts",
    "worker": "tsx src/temporal/worker.ts",
    "worker:dev": "tsx watch src/temporal/worker.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "echo 'No linter configured'"
  },
  "keywords": [
    "ai",
    "agent",
    "code-generation",
    "nextjs",
    "react",
    "typescript",
    "multi-agent",
    "llm"
  ],
  "author": "Agent Battalion Team",
  "license": "MIT",
  "dependencies": {
    "@pinecone-database/pinecone": "^2.0.0",
    "@temporalio/client": "^1.9.0",
    "@temporalio/worker": "^1.9.0",
    "@temporalio/workflow": "^1.9.0",
    "@temporalio/activity": "^1.9.0",
    "commander": "^12.0.0",
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "archiver": "^6.0.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "@types/archiver": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "@vitest/coverage-v8": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
